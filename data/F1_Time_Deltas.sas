PROC CORR DATA=GOLFSRS;
	VAR YEARBLT RATING;
RUN;

ODS GRAPHICS ON;
PROC REG DATA = RACE_QUALI PLOTS=DIAGNOSTICS(STATS=NONE);
	TITLE "CORRELATION: RACE_DELTA vs QUALI_DELTA";
	VAR RACE_DELTA QUALI_DELTA;
	MODEL RACE_DELTA =  QUALI_DELTA;
RUN;


